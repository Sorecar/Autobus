<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.servicio.com/autobus"
	xmlns:tns="http://www.servicio.com/autobus"
	elementFormDefault="qualified">

	<element name="ConsultarViajeRequest">
		<complexType>
			<sequence>
				<element name="origen" type="string"/>
				<element name="destino" type="string"/>
				<element name="fecha" type="date"/>
			</sequence>
		</complexType>
	</element>
	<element name="ConsultarViajeResponse">
		<complexType>
			<sequence>
				<element name="idViaje" type="string"/>
				<element name="origen" type="string"/>
				<element name="destino" type="string"/>
				<element name="fecha" type="date"/>
				<element name="hora" type="time"/>
				<element name="precio" type="int"/>
			</sequence>
		</complexType>
	</element>
	<element name="SeleccionarViajeRequest">
		<complexType>
			<sequence>
				<element name="idViaje" type="string"/>
			</sequence>
		</complexType>
	</element>
	<element name="SeleccionarViajeResponse">
		<complexType>
			<sequence>
				<element name="asientosDisponibles" type="int"/>
			</sequence>
		</complexType>
	</element>
	<element name="SeleccionarAsientoRequest">
		<complexType>
			<sequence>
				<element name="asiento" type="int"/>
				<element name="nomPasajero" type="string"/>
			</sequence>
		</complexType>
	</element>
	<element name="SeleccionarAsientoResponse">
		<complexType>
			<sequence>
				<element name="idBoleto" type="string"/>
				<element name="origen" type="string"/>
				<element name="destino" type="string"/>
				<element name="fecha" type="date"/>
				<element name="hora" type="time"/>
				<element name="precio" type="int"/>
				<element name="nomPasajero" type="string"/>
				<element name="asiento" type="int"/>
			</sequence>
		</complexType>
	</element>
	<element name="VerBoletoRequest">
		<complexType>
			<sequence>
				<element name="idBoleto" type="string"/>
			</sequence>
		</complexType>
	</element>
	<element name="VerBoletoResponse">
		<complexType>
			<sequence>
				<element name="idBoleto" type="string"/>
				<element name="origen" type="string"/>
				<element name="destino" type="string"/>
				<element name="fecha" type="date"/>
				<element name="hora" type="time"/>
				<element name="precio" type="int"/>
				<element name="nomPasajero" type="string"/>
				<element name="asiento" type="int"/>
			</sequence>
		</complexType>
	</element>
	<element name="ModificarBoletoRequest">
		<complexType>
			<sequence>
				<element name="idBoleto" type="string"/>
				<element name="nomPasajero" type="string"/>
				<element name="asiento" type="int"/>
			</sequence>
		</complexType>
	</element>
	<element name="ModificarBoletoResponse">
		<complexType>
			<sequence>
				<element name="idBoleto" type="string"/>
				<element name="origen" type="string"/>
				<element name="destino" type="string"/>
				<element name="fecha" type="date"/>
				<element name="hora" type="time"/>
				<element name="precio" type="int"/>
				<element name="nomPasajero" type="string"/>
				<element name="asiento" type="int"/>
			</sequence>
		</complexType>
	</element>
		<element name="CancelarBoletoRequest">
		<complexType>
			<sequence>
				<element name="idBoleto" type="string"/>
			</sequence>
		</complexType>
	</element>
		<element name="CancelarBoletoResponse">
		<complexType>
			<sequence>
				<element name="checkOut" type="string"/>
			</sequence>
		</complexType>
	</element>
	
</schema>